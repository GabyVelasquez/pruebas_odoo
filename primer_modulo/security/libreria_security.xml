<?xml version="1.0"?>
<odoo>
    <record id="categoria_modulo_libreria" model="ir.module.category">
        <field name="name">LibreriaE</field><!--Título del módulo para elegir el grupo de usuario-->
    </record>
    <record id="libreria_group_user" model="res.groups">
        <field name="name">AdministradorF</field><!--Título del tipo de usuario para el módulo-->
        <field name="category_id" ref="categoria_modulo_libreria"/>
        <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
    </record>
</odoo>